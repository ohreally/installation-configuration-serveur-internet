$ORIGIN example.edu.
$TTL 2d
@	IN	SOA	ns.example.com. sysadmin.example.com. (
				2020011501	; Numéro de série
				24h		; Renouveler
				2h		; Réessayer
				1000h		; Abolir
				2d		; TTL
			);
	IN	NS	ns.example.com.
	IN	NS	dns2.example.net.
	IN	MX	10 mx.example.com.
	IN	A	198.51.100.156
